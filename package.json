    "yaml": "^1.10.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.25",
    "cpy-cli": "^3.1.1",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  }
}
