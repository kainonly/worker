{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "weplanx/svc/(.*)": "<rootDir>/../libs/svc/src/$1",
    "weplanx/svc": "<rootDir>/../libs/svc/src",
    "@weplanx/data/(.*)": "<rootDir>/../libs/data/src/$1",
    "@weplanx/data": "<rootDir>/../libs/data/src",
    "@weplanx/database/(.*)": "<rootDir>/../libs/database/src/$1",
    "@weplanx/database": "<rootDir>/../libs/database/src"
  }
}